<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="bbr.b2b.logistic.dvrdeliveries.entities.TimeModule" table="TIMEMODULE" 
		entity-name="bbr.b2b.logistic.dvrdeliveries.entities.TimeModule" schema="LOGISTICA">
		<id name="id" type="java.lang.Long">
			<column name="ID" />
			<generator class="identity">
				<param name="sequence">TIMEMODULE_SEQUENCE</param>
				<param name="parameters">AS BIGINT CACHE 100</param>
			</generator>
		</id>

		<property name="description" type="java.lang.String" />
		<property name="visualorder" type="java.lang.Integer"/>
				
	</class>
</hibernate-mapping>
